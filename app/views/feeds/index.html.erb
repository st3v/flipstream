<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Name</th>
    <th>Description short</th>
    <th>Description long</th>
    <th>Keywords</th>
    <th>Itunes</th>
    <th>Artist</th>
    <th>Genre</th>
    <th>Category</th>
    <th>Subcategory</th>
    <th>Release date</th>
    <th>Crawl date</th>
    <th>Is video</th>
    <th>Explicit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed.url %></td>
    <td><%= feed.name %></td>
    <td><%= feed.description_short %></td>
    <td><%= feed.description_long %></td>
    <td><%= feed.keywords %></td>
    <td><%= feed.itunes_id %></td>
    <td><%= feed.artist_id %></td>
    <td><%= feed.genre_id %></td>
    <td><%= feed.category_id %></td>
    <td><%= feed.subcategory %></td>
    <td><%= feed.release_date %></td>
    <td><%= feed.crawl_date %></td>
    <td><%= feed.is_video %></td>
    <td><%= feed.explicit %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
